                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_centerVertical="true"
                            android:layout_marginEnd="10sp"
                            android:src="@drawable/ic_arrow_down" />
                    </RelativeLayout>

















                </LinearLayout>
                <Space
                    android:layout_width="10sp"
                    android:layout_height="match_parent" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"

                    >
                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/roboto_medium"
                        android:text="@string/iris_device_srno"

                        android:layout_marginTop="15dp"
                        android:textColor="@color/grayDark"
                        android:textSize="13dp" />









                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/input_serialno"

                        android:layout_width="match_parent"
                        android:layout_height="40sp"
                        android:layout_gravity="center"
                        android:background="@drawable/shape_rect_rounded_border_gray_layout"
                        android:fontFamily="@font/roboto_regular"
                        android:hint="@string/iris_device_srno"
                        android:paddingStart="10sp"
                        android:textSize="16sp"
                        android:gravity="center_vertical"
                        android:textColor="@color/black"
                        tools:ignore="RtlSymmetry" />

                </LinearLayout>

            </LinearLayout>




            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:visibility="visible"

            >

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto_medium"
                android:text="@string/select_date_and_time"
                android:layout_marginTop="15dp"
                android:textColor="@color/grayDark"
                android:textSize="13dp" />


                <EditText
                    android:id="@+id/dateAndTimeEditText"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_centerInParent="true"
                    android:focusable="false"
                    android:background="@drawable/shape_rect_rounded_border_gray_layout"
                    android:paddingStart="10sp"
                    android:onClick="showDateTimePicker"
                    android:hint="@string/date_and_time"
                    android:drawablePadding="20dp"
                    android:drawableRight="@drawable/ic_date_range"/>

        </LinearLayout>

            <!--  <androidx.appcompat.widget.AppCompatTextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:fontFamily="@font/roboto_medium"
                  android:text="@string/remark"
                  android:layout_marginTop="@dimen/_15sdp"
                  android:textColor="@color/grayDark"
                  android:textSize="13dp" />


              <TextView
                  android:id="@+id/inputRemark"
                  android:layout_width="match_parent"
                  android:layout_height="40sp"
                  android:layout_gravity="center"
                  android:background="@drawable/shape_rect_rounded_border_gray_layout"
                  android:fontFamily="@font/roboto_regular"
                  android:hint="@string/remark"
                  android:gravity="center_vertical"
                  android:paddingStart="10sp"
                  android:textSize="16sp"
                  tools:ignore="RtlSymmetry" />
  -->

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_remark"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/colorPrimary"
                android:padding="4dp"
               android:visibility="gone"
                android:text="Select Type"
                android:layout_marginTop="30dp"
                android:textSize="15dp"
                android:textColor="@color/white"
                android:fontFamily="@font/roboto_medium"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                />


<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:weightSum="2"
    android:layout_marginTop="10dp"
    android:visibility="gone">


    <CheckBox
        android:id="@+id/checkBox_wm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Weighing Machine"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/checkBox_iris"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Iris"
        android:visibility="invisible"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkBox"
        android:layout_weight="1"/>
</LinearLayout>



        <LinearLayout
            android:id="@+id/ll_wm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/colorPrimary"
                android:text="WeighingMachine"
                android:layout_marginTop="20dp"
                android:textSize="15dp"
                android:textColor="@color/white"
                android:fontFamily="@font/roboto_medium"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:padding="4dp"
                android:visibility="gone"

                />
        </LinearLayout>



            <LinearLayout
