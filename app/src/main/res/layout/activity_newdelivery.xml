                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"

                    >
                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/roboto_medium"
                        android:text="@string/fps_code"
                        android:layout_marginTop="15dp"
                        android:textColor="@color/grayDark"
                        android:textSize="13dp" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/inputFpsCode"
                        android:layout_width="match_parent"
                        android:layout_height="40sp"
                        android:layout_gravity="center"
                        android:background="@drawable/shape_rect_rounded_border_gray_layout"
                        android:fontFamily="@font/roboto_regular"
                        android:hint="FpsCode"
                        android:paddingStart="10sp"
                        android:textSize="16sp"
                        android:inputType="number"
                        android:textColor="@color/black"

                        tools:ignore="RtlSymmetry"
                        android:gravity="center_vertical"
                        />
                </LinearLayout>
                <Space
                    android:layout_width="10sp"
                    android:layout_height="match_parent" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"

                    >

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:text="@string/mobile_no"
                    android:layout_marginTop="15dp"
                    android:textColor="@color/grayDark"
                    android:textSize="13dp" />


                <androidx.appcompat.widget.AppCompatEditText
                    android:id="@+id/inputMobile"
                    android:layout_width="match_parent"
                    android:layout_height="40sp"
                    android:layout_gravity="center"
                    android:gravity="center_vertical"
                    android:background="@drawable/shape_rect_rounded_border_gray_layout"
                    android:fontFamily="@font/roboto_regular"
                    android:hint="@string/mobile_no"
                    android:paddingStart="10sp"
                    android:textSize="16sp"
                    android:inputType="number"
                    tools:ignore="RtlSymmetry"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    />
                </LinearLayout>

            </LinearLayout>





            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="visible"
                >


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:weightSum="2"

                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_weight="1"
                        >
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_medium"
                            android:layout_marginTop="15dp"
                            android:text="@string/weighing_machine_model"

                            android:textColor="@color/grayDark"
                            android:textSize="13dp" />
                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/intputWsModel"

                            android:layout_width="match_parent"
                            android:layout_height="40sp"
                            android:layout_gravity="center"
                            android:background="@drawable/shape_rect_rounded_border_gray_layout"
                            android:fontFamily="@font/roboto_regular"
                            android:hint="model"
                            android:paddingStart="10sp"
                            android:textSize="16sp"
                            android:inputType="number"
                            tools:ignore="RtlSymmetry"
                            android:gravity="center_vertical"
                            android:textColor="@color/black"

                            />





                    </LinearLayout>
                    <Space
                        android:layout_width="10sp"
                        android:layout_height="match_parent" />




                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_weight="1"
                        >
                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_medium"
                            android:text="@string/weighingmachine_serialno"

                            android:layout_marginTop="15dp"
                            android:textColor="@color/grayDark"
                            android:textSize="13dp" />
                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/inputWsSerialno"

                            android:layout_width="match_parent"
                            android:layout_height="40sp"
                            android:layout_gravity="center"
                            android:background="@drawable/shape_rect_rounded_border_gray_layout"
                            android:fontFamily="@font/roboto_regular"
                            android:hint="Serial No."
                            android:paddingStart="10sp"
                            android:textSize="16sp"
                            android:gravity="center_vertical"
                            android:textColor="@color/black"

                            android:inputType="number"
                            tools:ignore="RtlSymmetry" />

                    </LinearLayout>

                </LinearLayout>


            </LinearLayout>






            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:weightSum="2"
                android:visibility="gone"

                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"


                    >


                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/roboto_medium"
                        android:text="@string/iris_device_model"

                        android:layout_marginTop="15dp"
                        android:textColor="@color/grayDark"
                        android:textSize="13dp" />




                    <RelativeLayout
                        android:id="@+id/rl_model"
                        android:layout_width="match_parent"
                        android:layout_height="40sp"
                        android:visibility="visible"
                        android:layout_weight="1"
                        android:background="@drawable/shape_rect_rounded_border_gray_layout">

                        <androidx.appcompat.widget.AppCompatSpinner
                            android:id="@+id/inputmodel"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_toStartOf="@+id/iv_dropdownDistrict"
                            android:background="@null"
                            />

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/iv_dropdownDistrict"
