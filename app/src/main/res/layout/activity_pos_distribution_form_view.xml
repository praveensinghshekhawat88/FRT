<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.distributor.activity.PosDistributionFormViewActivity">
    <include
        android:id="@+id/action_bar"
        layout="@layout/global_action_bar" />

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/action_bar"
        android:fillViewport="true"
        android:scrollbars="none">
        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/top_lay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:orientation="vertical"
            android:padding="@dimen/_10sdp"
            android:visibility="visible">

            <ImageView
                android:layout_width="@dimen/_250sdp"
                android:layout_height="@dimen/_50sdp"
                android:layout_gravity="center"
                android:contentDescription="@string/app_name"
                android:src="@drawable/rcsc" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:fontFamily="@font/roboto_bold"
                android:gravity="center"
                android:text="@string/pos_installation_report"
                android:textColor="@color/black"
                android:textSize="@dimen/_16sdp" />

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/inputDate"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:enabled="false"
                            android:focusable="false"
                            android:fontFamily="@font/roboto_regular"
                            android:hint="@string/date_str"
                            android:paddingStart="@dimen/_5sdp"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:textColorHint="#757575"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="RtlSymmetry,TouchTargetSizeCheck,TouchTargetSizeCheck,TextContrastCheck" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <Space
                        android:layout_width="@dimen/_5sdp"
                        android:layout_height="match_parent" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/inputFpsCode"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:enabled="false"
                            android:fontFamily="@font/roboto_regular"
                            android:hint="@string/fps_code"
                            android:inputType="number"
                            android:paddingStart="@dimen/_5sdp"
                            android:textColor="@color/black"
                            android:textColorHint="#757575"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="RtlSymmetry,TextContrastCheck,TouchTargetSizeCheck" />
                    </com.google.android.material.textfield.TextInputLayout>
                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:orientation="horizontal">

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/inputDealerName"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:enabled="false"
                            android:fontFamily="@font/roboto_regular"
                            android:hint="@string/dealer_name"
                            android:paddingStart="@dimen/_5sdp"
                            android:textColor="@color/black"
                            android:textColorHint="#757575"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="RtlSymmetry,TouchTargetSizeCheck,TextContrastCheck" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <Space
                        android:layout_width="@dimen/_5sdp"
                        android:layout_height="match_parent" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/inputMobileNumber"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:enabled="false"
                            android:fontFamily="@font/roboto_regular"
                            android:hint="@string/mobile_no"
                            android:inputType="number"
                            android:maxLength="10"
                            android:paddingStart="@dimen/_5sdp"
                            android:textColor="@color/black"
                            android:textColorHint="#757575"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="RtlSymmetry,TextContrastCheck,TouchTargetSizeCheck" />
                    </com.google.android.material.textfield.TextInputLayout>
                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <com.callmangement.support.smartmaterialspinner.SmartMaterialSpinner
                        android:id="@+id/spinnerDistrict"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:enabled="false"
                        android:fontFamily="@font/roboto_regular"
                        app:smsp_floatingLabelColor="@color/grayDark"
                        app:smsp_hint="@string/district"
                        app:smsp_selectedItemListColor="@color/black" />

                    <Space
                        android:layout_width="@dimen/_5sdp"
                        android:layout_height="match_parent" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_weight="1">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/inputTicketNumber"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:enabled="false"
                            android:fontFamily="@font/roboto_regular"
                            android:hint="@string/ticket_number"
                            android:inputType="number"
                            android:paddingStart="@dimen/_5sdp"
                            android:textColor="@color/black"
                            android:textColorHint="#757575"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="RtlSymmetry,TextContrastCheck,TouchTargetSizeCheck" />
                    </com.google.android.material.textfield.TextInputLayout>
                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_weight="1">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/inputBlockName"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:enabled="false"
                            android:fontFamily="@font/roboto_regular"
                            android:hint="@string/block"
                            android:paddingStart="@dimen/_5sdp"
                            android:textColor="@color/black"
                            android:textColorHint="#757575"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="RtlSymmetry,TouchTargetSizeCheck,TextContrastCheck" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <Space
                        android:layout_width="@dimen/_5sdp"
                        android:layout_height="match_parent" />

                    <com.callmangement.support.smartmaterialspinner.SmartMaterialSpinner
                        android:id="@+id/spinnerEquipmentModel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:enabled="false"
                        app:smsp_floatingLabelColor="@color/grayDark"
                        app:smsp_hint="@string/equipment_model_str"
                        app:smsp_selectedItemListColor="@color/black" />
                </androidx.appcompat.widget.LinearLayoutCompat>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:fontFamily="@font/roboto_bold"
                    android:gravity="center"
                    android:text="@string/replacement_detail"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_14sdp" />

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/shape_rect_border_gray_layout"
                    android:orientation="vertical"
                    android:paddingTop="@dimen/_10sdp"
                    android:paddingBottom="@dimen/_10sdp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/roboto_regular"
                        android:paddingStart="@dimen/_10sdp"
                        android:paddingEnd="@dimen/_10sdp"
                        android:text="@string/whether_old_machine_provided_for_replacement"
                        android:textColor="@color/black" />

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:orientation="horizontal"
                        android:paddingStart="@dimen/_5sdp"
                        tools:ignore="RtlSymmetry">

                        <CheckBox
                            android:id="@+id/chkYes"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:checked="true"
                            android:enabled="false"
                            android:fontFamily="@font/roboto_regular"
                            android:text="@string/yes"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="TouchTargetSizeCheck" />

                        <Space
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="match_parent" />

                        <CheckBox
                            android:id="@+id/chkNo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:enabled="false"
                            android:fontFamily="@font/roboto_regular"
                            android:text="@string/no"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck" />
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>

                <com.callmangement.support.smartmaterialspinner.SmartMaterialSpinner
                    android:id="@+id/spinnerOldMachineMake"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:enabled="false"
                    android:fontFamily="@font/roboto_regular"
                    app:smsp_floatingLabelColor="@color/grayDark"
                    app:smsp_hint="@string/old_machine_make"
                    app:smsp_selectedItemListColor="@color/black" />

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/inputOldMachineSrNo"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:enabled="false"
                        android:fontFamily="@font/roboto_regular"
                        android:hint="@string/old_machine_sr_no"
                        android:imeOptions="actionNext"
                        android:paddingStart="@dimen/_5sdp"
                        android:singleLine="true"
                        android:textColor="@color/black"
                        android:textColorHint="#757575"
                        android:textSize="@dimen/_12sdp"
                        tools:ignore="RtlSymmetry,TextContrastCheck" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/inputFingerprintSrNo"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:enabled="false"
                        android:fontFamily="@font/roboto_regular"
                        android:hint="@string/fingerprint_sr_no"
                        android:imeOptions="actionNext"
                        android:paddingStart="@dimen/_5sdp"
                        android:singleLine="true"
                        android:textColor="@color/black"
                        android:textColorHint="#757575"
                        android:textSize="@dimen/_12sdp"
                        tools:ignore="RtlSymmetry,TextContrastCheck" />
                </com.google.android.material.textfield.TextInputLayout>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/shape_rect_border_gray_layout"
                    android:orientation="vertical"
                    android:paddingTop="@dimen/_10sdp"
                    android:paddingBottom="@dimen/_10sdp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/roboto_regular"
                        android:paddingStart="@dimen/_10sdp"
                        android:paddingEnd="@dimen/_10sdp"
                        android:text="@string/old_machine_condition"
                        android:textColor="@color/black" />

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:orientation="horizontal"
                        android:paddingStart="@dimen/_5sdp"
                        tools:ignore="RtlSymmetry">

                        <CheckBox
                            android:id="@+id/chkRunning"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:enabled="false"
                            android:fontFamily="@font/roboto_regular"
                            android:text="@string/running"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck" />

                        <Space
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="match_parent" />

                        <CheckBox
                            android:id="@+id/chkDead"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:enabled="false"
                            android:fontFamily="@font/roboto_regular"
                            android:text="@string/dead"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="TouchTargetSizeCheck" />
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>

                <com.callmangement.support.smartmaterialspinner.SmartMaterialSpinner
                    android:id="@+id/spinnerNewMachineMake"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:enabled="false"
                    android:fontFamily="@font/roboto_regular"
                    app:smsp_floatingLabelColor="@color/grayDark"
                    app:smsp_hint="@string/new_machine_make"
                    app:smsp_selectedItemListColor="@color/black" />

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/inputNewMachineOrderNo"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:enabled="false"
                        android:fontFamily="@font/roboto_regular"
                        android:hint="@string/new_machine_order_no"
                        android:imeOptions="actionNext"
                        android:inputType="number"
                        android:paddingStart="@dimen/_5sdp"
                        android:singleLine="true"
                        android:textColor="@color/black"
                        android:textColorHint="#757575"
                        android:textSize="@dimen/_12sdp"
                        tools:ignore="RtlSymmetry,TextContrastCheck" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/inputNewMachineSrNo"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:enabled="false"
                        android:fontFamily="@font/roboto_regular"
                        android:hint="@string/new_machine_sr_no"
                        android:imeOptions="actionNext"
                        android:paddingStart="@dimen/_5sdp"
                        android:singleLine="true"
                        android:textColor="@color/black"
                        android:textColorHint="#757575"
                        android:textSize="@dimen/_12sdp"
                        tools:ignore="RtlSymmetry,TextContrastCheck" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/inputNewFingerprintSrNo"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:enabled="false"
                        android:fontFamily="@font/roboto_regular"
                        android:hint="@string/new_fingerprint_sr_no"
                        android:imeOptions="actionNext"
                        android:paddingStart="@dimen/_5sdp"
                        android:singleLine="true"
                        android:textColor="@color/black"
                        android:textColorHint="#757575"
                        android:textSize="@dimen/_12sdp"
                        tools:ignore="RtlSymmetry,TextContrastCheck" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/inputIMEI_IMEI2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:enabled="false"
                        android:fontFamily="@font/roboto_regular"
                        android:hint="@string/imei1_imei2"
                        android:imeOptions="actionNext"
                        android:paddingStart="@dimen/_5sdp"
                        android:singleLine="true"
                        android:textColor="@color/black"
                        android:textColorHint="#757575"
                        android:textSize="@dimen/_12sdp"
                        tools:ignore="RtlSymmetry,TextContrastCheck" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:visibility="gone">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/inputAccessoriesProvided"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:enabled="false"
                        android:fontFamily="@font/roboto_regular"
                        android:hint="Accessories Provided"
                        android:imeOptions="actionNext"
                        android:paddingStart="@dimen/_5sdp"
                        android:singleLine="true"
                        android:textColor="@color/black"
                        android:textColorHint="#757575"
                        android:textSize="@dimen/_12sdp"
                        tools:ignore="RtlSymmetry,TextContrastCheck" />
                </com.google.android.material.textfield.TextInputLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:fontFamily="@font/roboto_bold"
                    android:gravity="center"
                    android:text="@string/call_end_status"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_14sdp" />

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/shape_rect_border_gray_layout"
                    android:orientation="vertical"
                    android:paddingTop="@dimen/_10sdp"
                    android:paddingBottom="@dimen/_10sdp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/roboto_regular"
                        android:paddingStart="@dimen/_10sdp"
                        android:paddingEnd="@dimen/_10sdp"
                        android:text="@string/complete_with_satisfactory"
                        android:textColor="@color/black" />

                    <CheckBox
                        android:id="@+id/chkCompleteWithSatisfactorily"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:checked="true"
                        android:enabled="false"
                        android:fontFamily="@font/roboto_regular"
                        android:text="@string/yes"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_12sdp"
                        tools:ignore="RtlSymmetry" />
                </androidx.appcompat.widget.LinearLayoutCompat>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:visibility="gone">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/inputRemark"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:enabled="false"
                        android:fontFamily="@font/roboto_regular"
                        android:hint="Remark (If Any)"
                        android:imeOptions="actionDone"
                        android:paddingStart="@dimen/_5sdp"
                        android:singleLine="true"
                        android:textColorHint="#757575"
                        android:textSize="@dimen/_12sdp"
                        tools:ignore="RtlSymmetry,TextContrastCheck" />
                </com.google.android.material.textfield.TextInputLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:weightSum="2">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:id="@+id/rl_upload_form_photo"
                            android:layout_width="70sp"
                            android:layout_height="70sp"
                            android:layout_gravity="center"
                            android:background="@drawable/bg_dashed_layout">

                            <ImageView
                                android:id="@+id/ivUploadFormPhoto"
                                android:layout_width="70sp"
                                android:layout_height="70sp"
                                android:adjustViewBounds="true"
                                android:contentDescription="@string/app_name"
                                android:scaleType="fitXY"
                                android:src="@drawable/ic_baseline_image" />
                        </RelativeLayout>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:fontFamily="@font/roboto_regular"
                            android:gravity="center"
                            android:text="@string/form_photo"
                            android:textColor="@color/black" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:id="@+id/rl_photo"
                            android:layout_width="70sp"
                            android:layout_height="70sp"
                            android:layout_gravity="center"
                            android:background="@drawable/bg_dashed_layout">

                            <ImageView
                                android:id="@+id/ivPhoto"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:adjustViewBounds="true"
                                android:contentDescription="@string/app_name"
                                android:scaleType="fitXY"
                                android:src="@drawable/ic_baseline_image" />
                        </RelativeLayout>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:fontFamily="@font/roboto_regular"
                            android:gravity="center"
                            android:text="@string/photo"
                            android:textColor="@color/black" />
                    </LinearLayout>

                </LinearLayout>


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:fontFamily="@font/roboto_regular"
