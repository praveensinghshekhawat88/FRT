    <style name="Theme.EHr" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>


    <style name="AppCompatAlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/teal_700</item>
        <item name="android:textColorPrimary">@color/teal_700</item>
        <item name="android:background">@color/white</item>
    </style>
    <!-- Theme.AppCompat.Light is not a dialog theme -->

    <style name="DialogTheme" parent="Theme.AppCompat.Light">
        <item name="colorAccent">@color/colorActionBar</item>

        <!--selected Item color-->
        <item name="colorControlActivated">#2196F3</item>
        <!-- arrow bordercolor(<>)-->
        <item name="android:selectableItemBackgroundBorderless">@color/teal_700</item>
        <!-- Highlight item color-->
        <item name="colorControlHighlight">#d50000</item>
        <!--Calender Background color -->
        <item name="android:windowBackground">@color/teal_200</item>
        <!-- Ok Cancel Color-->
        <item name="android:textColor">#4CAF50</item>
        <!-- Week TextColor-->
        <item name="android:textColorSecondary">#E91E63</item>
        <!-- Calender Number color arrow color (< >) -->
        <item name="android:textColorPrimary">#3F51B5</item>
        <!--day , month-->
        <item name="android:textColorPrimaryInverse">#0ff</item>
        <!-- year-->
        <item name="android:textColorSecondaryInverse">#009688</item>

    </style>


    <!--  <style name="DialogThemeTwo"  parent="android:Theme.Material.Dialog">
